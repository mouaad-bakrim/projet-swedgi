{% extends 'base.html.twig' %}


{% block title %}show client{% endblock %}

{% block body %}
    {{ form_start(form) }}
    <div class="row my-4">
        <div class="col-md-8 mx-auto">
            <div class="card">
                <div class="card-header">
                    show client
                </div>
                <div class="card-body">
                    {{ form_widget(form.email, {'attr': {'class': 'form-control'}}) }}

                </div>
            </div>
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}